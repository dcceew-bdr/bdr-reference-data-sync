@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix cs: <https://linked.data.gov.au/def/abis/themes> .
@prefix : <https://linked.data.gov.au/def/abis/themes/> .


cs: a skos:ConceptScheme ;
    skos:hasTopConcept :featureTypes ;
    skos:prefLabel "ABIS Themed-Vocabulary Themes"@en;
    .

:featureTypes a skos:Concept ;
    skos:prefLabel "ABIS Theme: Feature Types";
    skos:definition "Vocabularies with the dcat:theme featureType can be used where concepts go in the tern:featureType slot"@en;
    skos:topConceptOf cs: ;
    .

:samplingTypes a skos:Concept ;
    skos:prefLabel "ABIS Theme: Sampling Types";
    skos:definition "Vocabularies with the dcat:theme sampleType can be used where concepts go in the tern:samplingType or sdo:additionalType slot on a tern:Sampling or sosa:Sampling."@en;
    skos:topConceptOf cs: ;
    .

:observationTypes a skos:Concept ;
    skos:prefLabel "ABIS Theme: Observation Types";
    skos:definition "Vocabularies with the dcat:theme observationType can be used where concepts go in the tern:observationType or sdo:additionalType slot on a tern:Observation or sosa:Observation."@en;
    skos:topConceptOf cs: ;
    .
